<div class="search-bar row">
	<%= search_form_for @q do |f| %>
	<p class="columns form-input">
		<%= f.label :name_cont %>
		<%= f.text_field :name_cont %>
		</p>
	<p class="submit-button columns">
		<%= f.submit %>
	</p>
	<% end %>
</div>

<div class="row">
	<%= render "group_card_no_hidden", object: @groups %>
</div>